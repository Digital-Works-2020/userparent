<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>CreateUserServlet</servlet-name>
		<display-name>CreateUserServlet</display-name>
		<description></description>
		<servlet-class>com.digitalworks2020.user.servlets.CreateUserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DisplayUserListServlet</servlet-name>
		<display-name>DisplayUserListServlet</display-name>
		<description></description>
		<servlet-class>com.digitalworks2020.user.servlets.DisplayUserListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateUserServlet</servlet-name>
		<url-pattern>/CreateUserServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DisplayUserListServlet</servlet-name>
		<url-pattern>/DisplayUserListServlet</url-pattern>
	</servlet-mapping>
</web-app>
